    "zhms.client": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "skipTests": true,
          "style": "scss",
          "prefix": "hms"
        }
      },
      "root": "projects/zhms.client",
      "sourceRoot": "projects/zhms.client/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/zhms.client",
            "index": "projects/zhms.client/src/index.html",
            "browser": "projects/zhms.client/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/zhms.client/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/zhms.client/public"
              }
            ],
            "styles": [
              "projects/zhms.client/src/styles.scss",
              "node_modules/@angular/material/prebuilt-themes/indigo-pink.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "optimization": {
                "scripts": true,
                "styles": {
                  "minify": true,
                  "inlineCritical": false
                }
              },
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "10MB",
                  "maximumError": "50MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "10MB",
                  "maximumError": "50MB"
                }
              ],
              "outputHashing": "all",
              "serviceWorker": "projects/zhms.client/ngsw-config.json"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "zhms.client:build:production"
            },
            "development": {
              "buildTarget": "zhms.client:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/zhms.client/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/zhms.client/public"
              }
            ],
            "styles": [
              "projects/zhms.client/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
